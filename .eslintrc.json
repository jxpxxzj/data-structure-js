{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "semi": [2, "always"],
        "indent": [2,4],
        "no-path-concat": 0,
        "no-trailing-spaces": "error",
        "comma-dangle": "error",
        "comma-spacing": "error"
    }
}